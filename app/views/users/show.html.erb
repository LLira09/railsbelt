<a href="/songs">Dashboard</a>
<h2><%=@user.first_name%> <%=@user.last_name %>'s Playlist</h2>
<table>
    <thead>
      <th>Artist</th>
      <th>Title</th>
    </thead>
    <tr>
      <%@song.each do |song| %>
      <td><%=song.artist %></td>
      <td><%=song.title %></a></td>
      <td><%= song.playlists.count %></td>
    </tr>
    <% end %>
</table>
